        "@types/jsonwebtoken": "^9.0.2",
        "@types/mocha": "^10.0.1",
        "@types/multer": "^1.4.8",
        "@types/node": "^20.5.9",
        "@types/nodemailer": "^6.4.11",
        "@types/qrcode": "^1.5.2",
        "@types/speakeasy": "^2.0.7",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.3",
        "@types/ws": "^8.5.6",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "chai": "^4.3.8",
        "eslint": "^8.48.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.1",
        "pino-pretty": "^10.2.0",
