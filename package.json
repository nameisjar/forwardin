{
    "name": "expressjs-starter",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start": "node .",
        "dev": "nodemon .",
        "build": "node --max_old_space_size=4096 ./node_modules/typescript/bin/tsc",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "test": "mocha dist/tests/*.test.js --exit --timeout 10000",
        "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
    },
    "prisma": {
        "seed": "ts-node ./prisma/seed"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier --write",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "prettier --write",
            "git add"
        ]
    },
    "keywords": [],
    "author": "Amri Mufti (https://github.com/amrimuf)",
    "license": "MIT",
